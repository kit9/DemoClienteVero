<odoo>

    <!-- Menus -->
    <menuitem id="sunat_menu_root" name="Sunat" sequence="1" web_icon="sunat,static/description/icon.png"/>
    <menuitem id="sunat_detracciones_menu_category" name="Detracciones" sequence="1" parent="sunat_menu_root" action="action_sunat_detracciones"/>
    <!--menuitem id="action_sunat_detracciones_menu" name="Detracciones" sequence="1" parent="sunat_detracciones_menu_category" action="action_sunat_detracciones" /-->
    <menuitem id="sunat_document_type_menu_category" name="Tipo Documento" sequence="1" parent="sunat_menu_root" action="action_sunat_document_type" />

    <report
            id="account_invoices_prueba"
            model="account.invoice"
            string="Invoices Prueba"
            report_type="qweb-pdf"
            name="sunat.report_prueba"
            file="sunat.report_prueba"
            attachment="(object.state in ('open','in_payment','paid')) and ('INV'+(object.number or '').replace('/','')+'.pdf')"
            print_report_name="(object._get_report_base_filename())"
        />

</odoo>