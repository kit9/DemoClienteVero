<odoo>

    <!-- Form Tipo de Documentos -->
    <record id="customs_code_form_view" model="ir.ui.view">
        <field name="name">sunat.series.form.view</field>
        <field name="model">sunat.series</field>
        <field name="arch" type="xml">
            <form string="Form Tipo de Documento">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Thee -->
    <record id="customs_code_tree_view" model="ir.ui.view">
        <field name="name">sunat.series.tree.view</field>
        <field name="model">sunat.series</field>
        <field name="arch" type="xml">
            <tree string="Tree Tipo de Documentos">
                <field name="id"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- Action Document Type -->
    <record id="action_sunat_series" model="ir.actions.act_window">
        <field name="name">Tipo de Documento Sunat</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">sunat.series</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>